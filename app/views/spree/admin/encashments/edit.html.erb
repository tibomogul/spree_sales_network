<% content_for :page_title do %>
  <%= link_to Spree.t(:encashments), spree.admin_encashments_url %> /
  <%= @encashment.user.email %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @encashment } %>

<%= form_for [:admin, @encashment] do |form| %>
  <%= render partial: 'form', locals: { f: form } %>
  <%= render partial: 'spree/admin/shared/edit_resource_links', locals: { collection_url: spree.admin_encashments_path(@encashment) } %>
<% end %>
