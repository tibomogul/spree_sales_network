<div data-hook="admin_tax_rate_form_fields">
  <div class="panel panel-default" data-hook="tax_rates">

    <div class="panel-body">
      <div class="col-xs-12 col-md-6">
        <div data-hook="amount" class="form-group">
          <%= f.field_container :amount, class: ["form-group"] do %>
            <%= f.label :amount, Spree.t(:amount) + " (incl tax)" %>
            <%= f.number_field :amount, class: 'form-control' %>
            <%= f.error_message_on :amount %>
          <% end %>          
        </div>
        <div data-hook="withholding_tax" class="form-group">
          <%= f.field_container :withholding_tax, class: ["form-group"] do %>
            <%= f.label :withholding_tax, Spree.t(:withholding_tax) %>
            <%= f.number_field :withholding_tax, class: 'form-control' %>
            <%= f.error_message_on :withholding_tax %>
          <% end %>          
        </div>
      </div>
    </div>

  </div>
</div>